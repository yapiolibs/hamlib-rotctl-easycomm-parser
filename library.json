{
  "name": "HamlibRotctlEasycommParser",
  "keywords": "hamlib, rotctl, easycomm, parser, antenna, rotor",
  "description": "easycomm parser for hamlib rotator control",
  "homepage": "https://github.com/yapiolibs/hamlib-rotctl-easycomm-parser",
  "repository": {
    "type": "git",
    "url": "https://github.com/yapiolibs/hamlib-rotctl-easycomm-parser.git"
  },
  "authors": [{
    "name": "Raoul Rubien",
    "url": "https://github.com/rubienr",
    "maintainer": "true"
  }],
  "license": "Apache-2.0",
  "version": "0.1",
  "frameworks": ["native", "arduino"],
  "platforms": ["native", "atmelavr", "ststm32", "espressiv8266", "espressif32"],
  "examples": [{
      "name": "Parse a single command",
      "base": "test/src/",
      "files": [
        "platformio.ini",
        "example-parse-command.h",
        "example-parse-command.cpp"
      ]
    },
    {
      "name": "Parse a command with callback",
      "base": "test/src/",
      "files": [
        "platformio.ini",
        "example-parse-with-callback.h",
        "example-parse-with-callback.cpp"
      ]
    },
    {
      "name": "Parse data from stream",
      "base": "test/src/",
      "files": [
        "platformio.ini",
        "example-parse-stream.h",
        "example-parse-stream.cpp"
      ]
    }
  ],
  "export": {
    "exclude": [
      ".github/",
      "test/cmake*",
      "test/.idea"
    ]
  }
}
